  "kb:namingConventions": {
    "@id": "kb:naming-conventions-set",
    "@type": "kb:NamingConventions",
    "kb:comment": "Defines standard regex patterns for various naming contexts.",
    "kb:defaultFilePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[a-z0-9]+(-[a-z0-9]+)*$",
      "kb:description": "Default pattern for files (kebab-case)."
    },
    "kb:defaultDirectoryPattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[a-z0-9]+(-[a-z0-9]+)*$",
      "kb:description": "Default pattern for directories (kebab-case)."
    },
    "kb:kebabCasePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[a-z0-9]+(-[a-z0-9]+)*$",
      "kb:description": "Standard kebab-case pattern."
    },
    "kb:snakeCasePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[a-z0-9]+(_[a-z0-9]+)*$",
      "kb:description": "Standard snake_case pattern."
    },
    "kb:pascalCasePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[A-Z][a-zA-Z0-9]*$",
      "kb:description": "Standard PascalCase pattern."
    },
    "kb:camelCasePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[a-z][a-zA-Z0-9]*$",
      "kb:description": "Standard camelCase pattern."
    },
    "kb:upperSnakeCasePattern": {
      "@type": "kb:NamingRule",
      "kb:pattern": "^[A-Z0-9]+(_[A-Z0-9]+)*$",
      "kb:description": "Standard UPPER_SNAKE_CASE pattern."
    }
  },
  "kb:fieldDefinitions": [
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-title",
      "kb:fieldName": "title",
      "kb:description": "The official title of the document",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must not be empty"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-standard_id",
      "kb:fieldName": "standard_id",
      "kb:description": "A unique identifier for a standard document",
      "kb:mandatory": "conditional",
      "kb:dataType": "string",
      "kb:validationRules": [
        "MUST follow regex pattern: ^[A-Z]{2}-[A-Z]{2,6}-[A-Z0-9\\-]+$",
        "Filename (excluding .md extension) SHOULD be identical to standard_id"
      ],
      "kb:mandatoryConditions": [
        "Required for info-type: standard-definition, policy-document",
        "Optional for other document types, but recommended if document has canonical identifier"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-aliases",
      "kb:fieldName": "aliases",
      "kb:description": "A list of alternative names or titles by which the document might be known",
      "kb:mandatory": false,
      "kb:dataType": "list_of_strings",
      "kb:validationRules": [
        "None beyond being a list of strings"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-tags",
      "kb:fieldName": "tags",
      "kb:description": "A list of keywords or labels used to categorize the document",
      "kb:mandatory": true,
      "kb:dataType": "list_of_strings",
      "kb:validationRules": [
        "All tags MUST be in kebab-case",
        "MUST include tags from specific categories: status/*, content-type/*, topic/*",
        "Refer to mt-registry-tag-glossary.yaml for comprehensive list"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-kb_id",
      "kb:fieldName": "kb-id",
      "kb:description": "An identifier for the knowledge base this document belongs to",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must be in kebab-case",
        "Value must come from controlled vocabulary in mt-registry-tag-glossary.yaml"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-info_type",
      "kb:fieldName": "info-type",
      "kb:description": "Specifies the type or category of information the document represents",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must be in kebab-case",
        "MUST be one of the predefined values in controlled_vocabularies.info_type"
      ],
      "kb:hasControlledVocabulary": true
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-primary_topic",
      "kb:fieldName": "primary-topic",
      "kb:description": "A concise statement describing the main subject or purpose of the document",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must not be empty"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-related_standards",
      "kb:fieldName": "related-standards",
      "kb:description": "A list of other standards that are related to this document",
      "kb:mandatory": false,
      "kb:dataType": "list_of_strings",
      "kb:validationRules": [
        "Each string MUST be a valid standard_id or valid internal link format [[STANDARD_ID]]"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-version",
      "kb:fieldName": "version",
      "kb:description": "The version number of the document",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Semantic versioning preferred (e.g., '1.0.0', '0.2.1-alpha')"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-date_created",
      "kb:fieldName": "date-created",
      "kb:description": "The date and time when the document was originally created",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "MUST be in ISO-8601 date-time format: YYYY-MM-DDTHH:MM:SSZ"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-date_modified",
      "kb:fieldName": "date-modified",
      "kb:description": "The date and time when the document was last modified",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "MUST be in ISO-8601 date-time format: YYYY-MM-DDTHH:MM:SSZ"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-primary_domain",
      "kb:fieldName": "primary_domain",
      "kb:description": "The primary domain code (e.g., IT, HR, MT for Meta)",
      "kb:mandatory": "conditional",
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must be 2 uppercase letters",
        "Value MUST exist in controlled_vocabularies.primary_domain"
      ],
      "kb:hasControlledVocabulary": true,
      "kb:mandatoryConditions": [
        "Mandatory for standards documents",
        "Optional otherwise"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-sub_domain",
      "kb:fieldName": "sub_domain",
      "kb:description": "The sub-domain code (e.g., SECURITY, NETWORK, SCHEMA)",
      "kb:mandatory": "conditional",
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must be 2-6 uppercase letters",
        "Value MUST exist in controlled_vocabularies.sub_domain for the given primary_domain"
      ],
      "kb:hasControlledVocabulary": true,
      "kb:mandatoryConditions": [
        "Mandatory for standards documents",
        "Optional otherwise"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-scope_application",
      "kb:fieldName": "scope_application",
      "kb:description": "Defines the scope to which this document applies",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Must not be empty"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-criticality",
      "kb:fieldName": "criticality",
      "kb:description": "The criticality level of the document or the standard it defines",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Value MUST come from controlled_vocabularies.criticality",
        "Valid values: P0-Critical, P1-High, P2-Medium, P3-Low, P4-Informational"
      ],
      "kb:hasControlledVocabulary": true
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-lifecycle_gatekeeper",
      "kb:fieldName": "lifecycle_gatekeeper",
      "kb:description": "Specifies the role or team responsible for approving transitions in the document's lifecycle",
      "kb:mandatory": true,
      "kb:dataType": "string",
      "kb:validationRules": [
        "Value MUST come from controlled_vocabularies.lifecycle_gatekeeper",
        "Valid values: Architect-Review, Security-Team-Approval, Stakeholder-Review, No-Gatekeeper"
      ],
      "kb:hasControlledVocabulary": true
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-impact_areas",
      "kb:fieldName": "impact_areas",
      "kb:description": "A list of areas or systems that are affected by this document or standard",
      "kb:mandatory": true,
      "kb:dataType": "list_of_strings",
      "kb:validationRules": [
        "None beyond being a list of strings"
      ]
    },
    {
      "@type": "kb:FieldDefinition",
      "@id": "kb:field-change_log_url",
      "kb:fieldName": "change_log_url",
      "kb:description": "A URL or relative path pointing to the document's changelog",
      "kb:mandatory": "conditional",
      "kb:dataType": "string",
      "kb:validationRules": [
        "If relative path, MUST start with ./",
        "Linter SHOULD check for existence of linked file if relative path"
      ],
      "kb:mandatoryConditions": [
        "Mandatory for standards"
      ]
    }
  ],
